{
  "name": "quicker-employee-pp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "cache:clear": "rm -rf node_modules/.cache && rm -rf .vite && rm -rf node_modules/.vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest --runInBand -u",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test -- -u && npm run test:puppeteer"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-cache-adapter": "^2.7.3",
    "core-js": "^3.31.0",
    "core-js-pure": "^3.31.0",
    "inversify": "^6.0.1",
    "inversify-react": "^1.1.0",
    "react": "^18.2.0",
    "react-avatar": "^5.0.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-sort-class-members": "^1.17.1",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unicorn": "^46.0.0",
    "fake-indexeddb": "^4.0.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-axios": "^4.7.1",
    "jest-mock-extended": "^3.0.4",
    "jest-transform-stub": "^2.0.0",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "sass": "^1.62.0",
    "storybook": "^7.0.23",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-sass": "^0.1.0"
  }
}
